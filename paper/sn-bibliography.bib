	mesh = {Adolescent; Adult; Age Distribution; Comorbidity; Confidence Intervals; Contraceptive Agents, Female; Female; Humans; Incidence; Kaplan-Meier Estimate; Multivariate Analysis; Prognosis; Proportional Hazards Models; Prospective Studies; Risk Assessment; Severity of Illness Index; Sex Distribution; Sexually Transmitted Diseases; Vaginosis, Bacterial; Young Adult},
	month = {Aug},
	number = {2},
