	title = {Comparative meta-{RNA}-seq of the vaginal microbiota and differential expression by Lactobacillus iners in health and dysbiosis},
	volume = {1},
	year = {2013},
	bdsk-url-1 = {https://doi.org/10.1186/2049-2618-1-12}}

