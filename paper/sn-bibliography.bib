	number = {5},
	pages = {1812-9},
	pmc = {PMC2863870},
	pmid = {20305015},
	pst = {ppublish},
	title = {Quantitative {PCR} assessments of bacterial species in women with and without bacterial vaginosis},
	volume = {48},
